                                <td>
                                    @Html.DisplayFor(model => model.Address)
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    @Html.DisplayNameFor(model => model.Email)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => model.Email)
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    @Html.DisplayNameFor(model => model.PhonNumber)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => model.PhonNumber)
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    @Html.DisplayNameFor(model => model.CustomerSignature)
                                </td>
                                <td>
                                    <img src="@Url.Content(@Model.CustomerSignature)" alt="Sugnature" />
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="panel-group">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <h3 style="color:white">Device Detail</h3>
                    </div>
                    <div class="panel-body">
