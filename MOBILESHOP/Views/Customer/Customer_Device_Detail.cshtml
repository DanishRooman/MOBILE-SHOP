@model DataTransferObject.Customer_Device_Services.Customer_Device_ServicesDTO
@{
    ViewBag.Title = "Customer_Device_Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Customer_Device_Detail</h2>
<hr />
<div class="container">
    <div class="row">
